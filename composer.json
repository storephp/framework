{
    "name": "outmart/laravel-dashboard",
    "description": "eCommerce dashboard to help you manage your eCommerce based on laravel.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "ecommerce",
        "outmart",
        "dashboard"
    ],
    "authors": [
        {
            "name": "Karim Mohamed (Komtcho)",
            "email": "komicho1996@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=9.0",
        "livewire/livewire": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "Bidaea\\OutMart\\Dashboard\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bidaea\\OutMart\\Dashboard\\OutMartDashboardServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
